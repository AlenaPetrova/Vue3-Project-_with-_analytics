{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=0; includeSubDomains"
        },
        {
          "key": "Content-Security-Policy",
          "value": "upgrade-insecure-requests: false"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "http://109.73.206.144:6969/api/$1"
    }
  ],
  "env": {
    "HTTPS": "false",
    "VERCEL_FORCE_HTTPS": "false"
  }
}
